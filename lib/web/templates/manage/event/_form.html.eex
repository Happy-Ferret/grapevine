<%= form_for(@changeset, @url, fn f -> %>
  <%= FormView.text_field(f, :title) %>

  <%= FormView.textarea_field(f, :description, rows: 5) %>

  <%= FormView.text_field(f, :start_date) %>

  <%= FormView.text_field(f, :end_date) %>

  <div class="row">
    <div class="col-md-9 offset-md-3">
      <%= submit(@submit, class: "btn btn-primary") %>
    </div>
  </div>
<% end) %>
